<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiaoyn.xadmin.mapper.ConfigMapper">

	<select id="queryList" resultType="com.qiaoyn.xadmin.entity.ConfigEntity">
		select key_name ,value_name  from config where group_name = #{group} and status = 0
	</select>
</mapper>
